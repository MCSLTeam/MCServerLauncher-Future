<Page x:Class="MCServerLauncher.WPF.View.Pages.CreateInstancePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
      xmlns:h="clr-namespace:MCServerLauncher.WPF.Helpers"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CreateInstancePage">
    <Grid>
        <TextBlock Style="{StaticResource TitleTextBlock}" Text="{Binding [Main_CreateInstanceNavMenu], Source={x:Static h:LanguageManager.Instance}}" VerticalAlignment="Top"
                   HorizontalAlignment="Left" Margin="35,35,0,0" />
        <ui:Frame Margin="35,0,35,35" x:Name="CurrentCreateInstance" />
    </Grid>
</Page>